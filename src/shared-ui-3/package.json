{
  "name": "@niis/shared-ui",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts  --fix --ignore-path .gitignore",
    "license-check": "license-check-and-add check -f license-check-and-add-config.json",
    "license-add": "license-check-and-add add -f license-check-and-add-config.json",
    "npx-check-audit": "npx audit-ci@6.3.0 --config audit-ci-shared.json >build/reports/audit-ci.txt",
    "cleanup": "rm -rf dist && rm -rf package-lock.json && rm -rf node_modules && rm -rf xrd-shared-ui-3.0.0.tgz",
    "format": "prettier .  --write"
  },
  "main": "./dist/xrd-shared-ui.umd.js",
  "module": "./dist/xrd-shared-ui.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/xrd-shared-ui.js",
      "require": "./dist/xrd-shared-ui.umd.js"
    },
    "./dist/*.css": {
      "import": "./dist/*.css",
      "require": "./dist/*.css"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "@fontsource/open-sans": "^5.0.3",
    "@mdi/font": "^7.2.96",
    "deepmerge": "^4.3.1"
  },
  "peerDependencies": {
    "vue": "^3.3.4",
    "vue-i18n": "^9.2.2",
    "vuetify": "^3.3.9"
  },
  "devDependencies": {
    "@intlify/eslint-plugin-vue-i18n": "^2.0.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^18.16.17",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.15.0",
    "eslint-plugin-vuetify": "^2.0.2",
    "license-check-and-add": "~4.0.5",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.8",
    "sass": "^1.63.4",
    "typescript": "~5.1.3",
    "vite": "^4.4.6",
    "vite-plugin-dts": "^3.3.1",
    "vite-plugin-vuetify": "^1.0.2",
    "vue": "^3.3.4",
    "vue-eslint-parser": "^9.3.1",
    "vue-tsc": "^1.6.5"
  },
  "overrides": {
    "semver": "^7.5.3"
  }
}
